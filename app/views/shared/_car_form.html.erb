<div class="col-sm-12 col-md-6 col-md-offset-3">
  <%= simple_form_for @car do |f| %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :year %>
    <%= f.input :mileage %>
    <%= f.input :color %>
    <%= f.input :price_per_day %>
    <%= f.input :location %>
    <p>upload photo to implement</p>
    <%= f.submit class: "btn btn-danger"%>
  <% end %>
</div>
