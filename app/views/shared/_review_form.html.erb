
    <%= simple_form_for [@car,@review] do |f| %>
      <div class="booking-form">
        <%= f.input :content, placeholder: "Write a review" %>
        <%= f.input :rating, placeholder: "Give a rating between 1 & 5", collection: (1..5) %>
        <%= f.submit class: "btn btn-danger" %>
      </div>
    <% end %>

